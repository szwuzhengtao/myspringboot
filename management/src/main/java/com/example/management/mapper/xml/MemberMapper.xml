<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.management.mapper.MemberMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.management.pojo.po.Member">
        <id column="memberQQ" property="memberQQ" />
        <result column="nickname" property="nickname" />
        <result column="isCustomer" property="isCustomer" />
        <result column="avatarURL" property="avatarURL" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        memberQQ, nickname, isCustomer, avatarURL
    </sql>

</mapper>
