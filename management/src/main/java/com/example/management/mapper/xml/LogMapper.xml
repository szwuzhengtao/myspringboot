<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.management.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.management.pojo.Log">
        <id column="logId" property="logId" />
        <result column="operation" property="operation" />
        <result column="staffId" property="staffId" />
        <result column="staffName" property="staffName" />
        <result column="customerId" property="customerId" />
        <result column="customerName" property="customerName" />
        <result column="time" property="time" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        logId, operation, staffId, staffName, customerId, customerName, time
    </sql>

</mapper>
